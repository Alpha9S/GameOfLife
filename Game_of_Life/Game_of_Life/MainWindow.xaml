<Window x:Class="Game_of_Life.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game_of_Life"
        mc:Ignorable="d"
        Title="MainWindow" Height="1000" Width="1200">
    <DockPanel>
        
        <Grid Name="grid" DockPanel.Dock="Right" Width="1000"/>
        <DockPanel DockPanel.Dock="Left" LastChildFill="False">
            <Button Height="50" DockPanel.Dock="Top">Timer</Button>
            <Button Height="50" DockPanel.Dock="Top">Start / Stop</Button>
        </DockPanel>
    </DockPanel>
    
</Window>
